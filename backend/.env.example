# LiteLLM Proxy Configuration (from llmops_lite)
LITELLM_PROXY_BASE_URL=https://your-litellm-proxy.com
LITELLM_PROXY_SECRET_KEY=your-secret-key-here

# AWS Configuration (Optional - for DynamoDB caching)
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1
DEFAULT_CACHE_TABLE_NAME=llm-cache

# Application Settings
APP_ENV=development
DEBUG=True
LOG_LEVEL=INFO

# ChromaDB Settings
CHROMA_PERSIST_DIRECTORY=../chroma_data
CHROMA_COLLECTION_NAME=insurance_policies

# API Settings
API_HOST=0.0.0.0
API_PORT=8000
CORS_ORIGINS=http://localhost:3000,https://your-frontend.vercel.app

# llmops_lite Settings
SHOW_CACHE_LOGS=false
SHOW_PRINTS=false
USE_PARALLEL_OPTIMIZATION=true
